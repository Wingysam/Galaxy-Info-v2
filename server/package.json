{
  "name": "galaxy-info",
  "version": "2.0.0",
  "description": "Galaxy Info is a Discord bot for information of Galaxy",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext ts --fix",
